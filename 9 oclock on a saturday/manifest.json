{
	"manifest_version": 2,
	"name": "9 O'Clock on a Saturday",
	"version": "1.0",
	"description": "Opens a tab with Billy Joel's 'Piano Man' every Saturday at 9 PM.",
	"icons": {"48": "icons/icon-48.png", "96": "icons/icon-96.png" },
	"permissions": [
		"alarms",
		"bookmarks",
		"tabs"
	],
	"browser_action": {
		"default_icon": "icons/icon-96.bmp",
		"default_title": "piano man"
	},
	"background": {
		"scripts": ["9 oclock on a saturday.js"]
	}
}